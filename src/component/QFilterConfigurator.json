{
  "type": "component",
  "props": {
    "value": {
      "type": [
        "Object"
      ],
      "desc": "Model of the component; Either use this property (along with a listener for 'input' event) OR use v-model directive",
      "required": true,
      "examples": [
        "v-model=\"myText\""
      ],
      "category": "model"
    },
    "nodes": {
      "type": [
        "Array"
      ],
      "desc": "Available nodes that the user can select from",
      "required": true,
      "examples": [
        ":nodes=\"[ { label: 'General', icon: 'person', options: \"[ { label: 'Samsung Phone', value: 'phone' } ]\" } ]\"",
        ":nodes=\"[ { label: 'Orders', icon: 'shopping_cart', multiple: true, options: \"[ 'delivered', 'arrived' ]\" } ]\""
      ],
      "category": "nodes"
    },
    "ignore": {
      "type": [
        "Array"
      ],
      "desc": "List of filters that will be ignored",
      "examples": [
        ":ignore=\"[ 'custom_filter' ]\""
      ],
      "default": "[]",
      "category": "nodes"
    },
    "reverse": {
      "type": [
        "Boolean"
      ],
      "desc": "Reverse row",
      "category": "style"
    },
    "color": {
      "type": [
        "String"
      ],
      "desc": "Color name for component from the Quasar Color Palette. Except brand colors!",
      "examples": [
        "red",
        "pink",
        "purple",
        "indigo",
        "blue",
        "light-blue",
        "cyan",
        "teal",
        "green",
        "light-green",
        "lime",
        "amber",
        "orange",
        "brown",
        "grey"
      ],
      "default": "blue",
      "category": "style"
    },
    "maxDisplay": {
      "type": [
        "Number",
        "String"
      ],
      "desc": "The maximum values to shown in the QChip",
      "default": 5,
      "category": "style"
    },
    "propertiesLabel": {
      "type": [
        "String"
      ],
      "desc": "Default 'properties' label",
      "default": "filter(s) available",
      "category": "content"
    },
    "dateFormat": {
      "type": [
        "String"
      ],
      "desc": "Date format for 'date' filters",
      "default": "DD/MM/YY",
      "category": "style"
    }
  },
  "events": {
    "input": {
      "desc": "Emitted when the component needs to change the model; Is also used by v-model",
      "params": {
        "value": {
          "desc": "New model value",
          "type": [
            "Object"
          ],
          "required": true
        }
      }
    }
  }
}